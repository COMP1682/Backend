{
    "version": 1,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    
    "routes": [
        {
          "src": "/(.*)",
          "dest": "dist/(.*)",
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
          "headers": {
            "Access-Control-Allow-Origin": "*"
          }
        }
      ]
}